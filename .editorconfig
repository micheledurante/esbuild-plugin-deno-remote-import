# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

# Matches multiple files with brace expansion notation
# Set default charset
[*.{yml,json,yaml}]
indent_size = 2
indent_style = space
charset = utf-8

[*.{js,ts}]
indent_size = 4
indent_style = space
charset = utf-8
